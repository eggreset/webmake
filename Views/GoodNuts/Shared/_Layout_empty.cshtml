@using MyTool.ViewModels
@using WebMaker.BLL.Helpers
@using WabMaker.Web.Helpers
@{
    SiteInfoViewModel SiteInfo = ApplicationHelper.SiteInfo;
}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    @if (SiteInfo != null)
    {
        @Html.Partial("_BaseMeta", SiteInfo)
    }
    <link rel="icon" href="~/html/ohmygoodnuts/img/favicon.png" type="image/png" />
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/bootstrap.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/linericon/style.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/themify-icons.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/owl-carousel/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/lightbox/simpleLightbox.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/nice-select/css/nice-select.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/animate-css/animate.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/vendors/jquery-ui/jquery-ui.css" />
    <!-- main css -->
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/style.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/responsive.css" />
    <link rel="stylesheet" href="~/html/ohmygoodnuts/css/main.css?v=2">

    @RenderSection("css", required: false)
    @RenderSection("meta", required: false)
</head>

<body>

    @RenderBody()
    
    @Scripts.Render("~/bundles/jquery")
    @Html.Partial("_ShowMessage")
    @Html.Partial("_BaseScripts")
    <script src="~/html/ohmygoodnuts/js/bootstrap.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>